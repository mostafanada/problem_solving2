#include <bits/stdc++.h>

using namespace std;

int main()
{
    int t;cin>>t;
    while(t--)
    {
        int n;cin>>n;
        long long s=0;
        int arr[n];
        for(int i=0;i<n;i++)cin>>arr[i];
        for(int i=0;i<n-1;i++)
        {
            long long a=max(arr[i],arr[i+1]);
            long long b=min(arr[i],arr[i+1]);
            a*=b;
            s=max(s,a);
        }
        cout<<s<<"\n";
    }
    return 0;
}
